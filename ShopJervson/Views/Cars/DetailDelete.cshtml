@model ShopJervson.Models.Car.CarDetailDeleteViewModel
<h1>DetailDelete</h1>
<div>

    <h4>Car</h4>
    <hr />
    <dl class="row">
        <div class="form-group-row">
            <label asp-for="Id" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Id)
        </div>

        <div class="form-group-row">
            <label asp-for="Brand" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Brand)
        </div>

        <div class="form-group-row">
            <label asp-for="YearBuilt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.YearBuilt)
        </div>

        <div class="form-group-row">
            <label asp-for="Color" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.Color)
        </div>

        <div class="form-group-row">
            <label asp-for="ZeroToHundred" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.ZeroToHundred)
        </div>

        <div class="form-group-row">
            <label asp-for="FuelConsumption" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.FuelConsumption)
        </div>

        <div class="form-group-row">
            <label asp-for="ModifiedAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.ModifiedAt)
        </div>

        <div class="form-group-row">
            <label asp-for="CreatedAt" class="col-sm-2 col-form-label"></label>
            @Html.DisplayFor(model => model.CreatedAt)
        </div>
        @foreach (var images in Model.FileToApiViewModels)
        {
            <partial name="_Images" model="images" />
        }
    </dl>
</div>
<hr/>
    <form asp-action="DeleteConfirmation">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="index" asp-controller="cars" class="btn btn-primary">Back</a>
    </form>
